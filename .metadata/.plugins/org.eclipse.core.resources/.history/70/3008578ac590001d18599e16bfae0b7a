package ejercicio1;

public class ejercicio1 implements Runnable {

	private int numeroInicial;

	private int resultado;

	public ejercicio1(int numeroInicial) {

		super();

		this.numeroInicial = numeroInicial;

	}

	public void multiplicar() {

		System.out.println("multiplicacion");
//numeroInicial *=(int)Math.floor(Math.random()*10+1);
		resultado = numeroInicial * 7 * 8 * 9;
	//	System.out.println("El resultado es: " + resultado);
		//numeroInicial=1;

	}

	@Override

	public void run() {

		this.multiplicar();

	}

	public static void main(String[] args) {

		ejercicio1 ej1 = new ejercicio1(1);

		for (int i = 0; i < 10; i++) {

			new Thread(ej1).start();

		}

	}

}
